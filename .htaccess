RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule !/$ %{REQUEST_URI}/ [L,R=301]

RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

RewriteRule ^([^/]+)/([^/]+)/?$ index.php?phone=$1&vrh=$2&vrh=$3 [L,QSA]

RewriteRule ^([^/]+)/?$ index.php?PID=$1?phone=$1&vrh=$2&vrh=$3 [L,QSA]